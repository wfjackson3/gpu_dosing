<h1>Listing searches</h1>

<table>
  <tr>
    <th>Line</th>
    <th>Flow</th>
    <th>Pressure</th>
    <th>Head</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @searches.each do |search| %>
  <tr>
    <td><%= search.line_id %></td>
    <td><%= search.flow %></td>
    <td><%= search.pressure %></td>
    <td><%= search.head_id %></td>
    <td><%= link_to 'Show', search %></td>
    <td><%= link_to 'Edit', edit_search_path(search) %></td>
    <td><%= link_to 'Destroy', search, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Search', new_search_path %>
